@model AP5PW_Helpdesk.ViewModels.CompanyVM
@{
    ViewData["Title"] = "Company details";
}
<h2>@ViewData["Title"]</h2>

<dl class="row">
    <dt class="col-sm-2">Id</dt>
    <dd class="col-sm-10">@Model.Id</dd>

    <dt class="col-sm-2">Username</dt>
    <dd class="col-sm-10">@Model.Name</dd>

    <dt class="col-sm-2">City</dt>
    <dd class="col-sm-10">@Model.City</dd>

    <dt class="col-sm-2">Street</dt>
    <dd class="col-sm-10">@Model.Street</dd>

    <dt class="col-sm-2">Postcode</dt>
    <dd class="col-sm-10">@Model.Postcode</dd>
</dl>

<a asp-action="Edit" asp-route-id="@Model.Id" class="btn btn-primary">Edit</a>
<a asp-action="Index" class="btn btn-secondary ms-2">Back</a>
